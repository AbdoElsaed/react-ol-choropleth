{
  "name": "react-ol-choropleth",
  "version": "0.0.1",
  "private": false,
  "description": "A React plugin for creating choropleth maps using OpenLayers",
  "type": "module",
  "main": "dist/react-ol-choropleth.umd.js",
  "module": "dist/react-ol-choropleth.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-ol-choropleth.es.js",
      "require": "./dist/react-ol-choropleth.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:demo": "vite build --config vite.demo.config.ts",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "ol": "^8.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "chroma-js": "^2.4.2",
    "d3-scale": "^4.0.2",
    "ol": "^8.0.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.1",
    "@types/chroma-js": "^2.4.3",
    "@types/d3-scale": "^4.0.8",
    "@types/node": "^22.13.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^1.3.1"
  },
  "keywords": [
    "react",
    "openlayers",
    "choropleth",
    "map",
    "visualization",
    "geospatial",
    "gis"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/react-ol-choropleth"
  },
  "bugs": {
    "url": "https://github.com/yourusername/react-ol-choropleth/issues"
  },
  "homepage": "https://github.com/yourusername/react-ol-choropleth#readme"
}
